{
  "name": "generator-webpack-react-jest",
  "version": "0.0.1",
  "description": "It's simplify generator for React applications, with support Jest as test runner and Enzyme",
  "scripts": {
    "postinstall": "lerna bootstrap",
    "lint": "eslint ./generators/* --ext .js --ext .jsx --ignore-path .gitignore",
    "pretest": "yarn lint --cache",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khaninD/generator-react-webpack-jest.git"
  },
  "author": "Khanin Daniil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khaninD/generator-react-webpack-jest/issues"
  },
  "homepage": "https://github.com/khaninD/generator-react-webpack-jest#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "lerna": "^2.4.0",
    "webpack": "^3.6.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "mkdir": "0.0.2",
    "ncp": "^2.0.0",
    "underscore.string": "^3.3.4",
    "yeoman-generator": "^2.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "test/**/*.js"
    ],
    "roots": [
      "<rootDir>/test"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ]
  }
}
